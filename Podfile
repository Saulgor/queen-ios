  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  pod 'SDWebImage'
  pod 'SwiftyJSON'
  pod 'SocketRocket'
  

  # Pods for queen-ios

  target 'queen-iosTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'queen-iosUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
